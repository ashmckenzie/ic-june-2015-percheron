upstream app_servers {
  hash $request_uri consistent;
  {{range service "app.demo-app"}}
  server {{.Address}}:{{.Port}}; # {.Node}{{end}}
}

server {
  listen 80;
  server_name _;

  proxy_set_header Host $host;

  add_header X-Proxy-Hostname $hostname;

  location / {
    proxy_pass http://app_servers;
  }
}
